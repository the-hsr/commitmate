{
  "name": "commitmate",
  "displayName": "CommitMate",
  "version": "0.0.1",
  "publisher": "thehsr",
  "description": "An AI-powered VS Code extension that analyzes your staged Git diffs and meaningful. concise commit messages.",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "commitmate.generateCommitMessage",
        "title": "CommitMate: Generate Commit Message"
      }
    ]
  },
  "dependencies": {
    "groq-sdk": "^0.30.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.5.0"
  }
}
